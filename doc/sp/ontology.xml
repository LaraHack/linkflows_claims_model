<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/linkflows/superpattern/terms/SP-Ontology#"
     xml:base="https://w3id.org/linkflows/superpattern/terms/SP-Ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/linkflows/superpattern/terms/SP-Ontology">
        <terms:creator rdf:resource="https://orcid.org/0000-0002-1267-0234"/>
        <terms:creator rdf:resource="https://orcid.org/0000-0002-7114-6459"/>
        <terms:description>This is an ontology of the SuperPattern with which the main claims of the scientific articles can be expressed.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/publicdomain/zero/1.0/"/>
        <terms:title>SuperPattern Ontology</terms:title>
        <vann:preferredNamespacePrefix>sp</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/linkflows/superpattern/terms/</vann:preferredNamespaceUri>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/SP-Ontology"/>
        <rdfs:seeAlso rdf:resource="https://github.com/LaraHack/linkflows_model/blob/master/Linkflows.ttl#Linkflows"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://xmlns.com/foaf/0.1/member -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/member"/>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/affects -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/affects">
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/PotentialActivity"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/affects"/>
        <rdfs:label xml:lang="en">affects</rdfs:label>
        <skos:definition xml:lang="en">[subj] affects the activity of [obj] in the sense of positively or negatively affecting its intensity or frequency</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/causes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/causes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/causes"/>
        <rdfs:label xml:lang="en">causes</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the existence of [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/coOccursWith -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/coOccursWith">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/coOccursWith"/>
        <rdfs:label xml:lang="en">co occurs with</rdfs:label>
        <skos:definition xml:lang="en">[subj] is close to [obj] in terms of space and time</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/comparesTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/comparesTo">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/comparesTo"/>
        <rdfs:label xml:lang="en">compares to</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] compares to the [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/contributesTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/contributesTo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/affects"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/PotentialActivity"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/contributesTo"/>
        <rdfs:label xml:lang="en">contributes to</rdfs:label>
        <skos:definition xml:lang="en">[subj] contributes to the activity of [obj] in sense of positively affecting its intensity or frequency</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/decreases -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/decreases">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/decreases"/>
        <rdfs:label xml:lang="en">decreases</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the value of [obj] to decrease</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/enables -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/enables">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/contributesTo"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/PotentialActivity"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/enables"/>
        <rdfs:label xml:lang="en">enables</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the activity to happen</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/follows -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/follows">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/follows"/>
        <rdfs:label xml:lang="en">follows</rdfs:label>
        <skos:definition xml:lang="en">[obj] comes into existence before [subj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/frequentlyNotQualifier -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/frequentlyNotQualifier">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasCausalRelationWith"/>
        <rdfs:label xml:lang="en">has causal relation with</rdfs:label>
        <skos:definition xml:lang="en">[subj] has causal relation with [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasContextClass -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasContextClass">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasContextClass"/>
        <rdfs:label xml:lang="en">has context class</rdfs:label>
        <skos:definition xml:lang="en">links a superpattern instance to its context class</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasDifferentValueFrom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasDifferentValueFrom">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/comparesTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasDifferentValueFrom"/>
        <rdfs:label xml:lang="en">has different value from</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] has a value that is different from [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasLargerValueThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasLargerValueThan">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasDifferentValueFrom"/>
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasLargerValueThan"/>
        <rdfs:label xml:lang="en">has a larger value than</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] has a larger value than [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasObjectClass -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasObjectClass">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasObjectClass"/>
        <rdfs:label xml:lang="en">has object class</rdfs:label>
        <skos:definition xml:lang="en">links a superpattern instance to its object class</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasQualifier -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasQualifier">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternInstance"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternQualifier"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasQualifier"/>
        <rdfs:label xml:lang="en">has qualifier</rdfs:label>
        <skos:definition xml:lang="en">links a subject class from a superpattern instance to its qualifier class</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasRelation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasRelation">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasRelation"/>
        <rdfs:label xml:lang="en">has relation</rdfs:label>
        <skos:definition xml:lang="en">links a subject class to an object class in a superpattern instance</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasSameValueAs -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasSameValueAs">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSimilarValueAs"/>
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasSameValueAs"/>
        <rdfs:label xml:lang="en">has same value as</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] has the same value as [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasSimilarValueAs -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasSimilarValueAs">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/comparesTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasSimilarValueAs"/>
        <rdfs:label xml:lang="en">has similar value as</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] has a value that is similar with [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasSmallerValueThan -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasSmallerValueThan">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasDifferentValueFrom"/>
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasSmallerValueThan"/>
        <rdfs:label xml:lang="en">has a smaller value than</rdfs:label>
        <skos:definition xml:lang="en">[quantifiable-subj] has a smaller value than [quantifiable-obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith">
        <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/member"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasSpatiotemporalRelationWith"/>
        <rdfs:label xml:lang="en">has spatio-temporal relation with</rdfs:label>
        <skos:definition xml:lang="en">[subj] has a spatial and/or temporal relation with [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/hasSubjectClass -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/hasSubjectClass">
        <rdfs:domain rdf:resource="https://w3id.org/linkflows/superpattern/terms/SuperPatternInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/hasSubjectClass"/>
        <rdfs:label xml:lang="en">has subject class</rdfs:label>
        <skos:definition xml:lang="en">links a superpattern instance to its subject class</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/includes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/includes">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/includes"/>
        <rdfs:label xml:lang="en">includes</rdfs:label>
        <skos:definition xml:lang="en">[subj] spatiotemporally includes [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/increases -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/increases">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/QuantifiableThing"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/increases"/>
        <rdfs:label xml:lang="en">increases</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the value of [obj] to increase</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/inhibits -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/inhibits">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/affects"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/PotentialActivity"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/inhibits"/>
        <rdfs:label xml:lang="en">inhibits</rdfs:label>
        <skos:definition xml:lang="en">[subj] inhibits the activity of [obj] in sense of negatively affecting its intensity or frequency</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isCausedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isCausedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/causes"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isCausedBy"/>
        <rdfs:label xml:lang="en">is caused by</rdfs:label>
        <skos:definition xml:lang="en">the existence of [subj] is caused by [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isFollowedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isFollowedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isFollowedBy"/>
        <rdfs:label xml:lang="en">is followed by</rdfs:label>
        <skos:definition xml:lang="en">[subj] comes into existence before [obj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isInContextOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isInContextOf">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isInContextOf"/>
        <rdfs:label xml:lang="en">is in context of</rdfs:label>
        <skos:definition xml:lang="en">links subject and object instances to context instances</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isIncludedIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isIncludedIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasSpatiotemporalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isIncludedIn"/>
        <rdfs:label xml:lang="en">is included in</rdfs:label>
        <skos:definition xml:lang="en">[obj] spatiotemporally includes [subj]</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isNecessaryAndSufficientFor -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isNecessaryAndSufficientFor">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/causes"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isNecessaryAndSufficientFor"/>
        <rdfs:label xml:lang="en">is necessary and sufficient for</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the existence of [obj], and [obj] would not exist if [subj] did not exist</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/isSameAs -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/isSameAs">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/isSameAs"/>
        <rdfs:label xml:lang="en">is same as</rdfs:label>
        <skos:definition xml:lang="en">[subj] and [obj] are the same individual</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/prevents -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/prevents">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/inhibits"/>
        <rdfs:range rdf:resource="https://w3id.org/linkflows/superpattern/terms/PotentialActivity"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/prevents"/>
        <rdfs:label xml:lang="en">prevents</rdfs:label>
        <skos:definition xml:lang="en">[subj] causes the activity [obj] not to happen</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/linkflows/superpattern/terms/requires -->


    <owl:ObjectProperty rdf:about="https://w3id.org/linkflows/superpattern/terms/requires">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/linkflows/superpattern/terms/hasCausalRelationWith"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/linkflows/superpattern/latest/requires"/>
